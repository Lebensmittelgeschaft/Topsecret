{
  "name": "topsecret-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.42",
    "material-ui": "^1.0.0-beta.41",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-relay": "^1.5.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "relay-runtime": "^1.5.0"
  },
  "scripts": {
    "start": "npm run relay-conf && react-scripts start",
    "build": "npm run relay-conf && react-scripts build",
    "test": "npm run relay-conf && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "incl-babel": "node ./scripts/babelPluginInclude.js",
    "compile-relay": "relay-compiler --src ./src --schema ./schema/schema.graphql --extensions=jsx --extensions=js",
    "relay-conf": "npm run incl-babel && npm run compile-relay"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.5.0",
    "flow-bin": "^0.69.0",
    "relay-compiler": "^1.5.0"
  }
}
